<sci:data xmlns:sci="http://www.SciTegic.com/" object="SciTegic.Protocol.1" format="8.0.0">
	<sci:dbitem>
		<sci:component name="Phylogenetic Tree and MSA" version="2">
			<sci:arg name="Script" type="TextType" required="true" visibility="visible" multi="false">
				<sci:help>Text of the R script you wish to run.  Enter the script just as if you were typing it in R.</sci:help>
				<sci:value>#required for msa
library(msa)
library(seqinr)
library(ape)



#input sequences into AA format
Fasta &lt;- AAStringSet(as.matrix(AllData[c(&quot;sequence&quot;)]))
names(Fasta) &lt;- as.matrix(AllData[c(&quot;$(Annotate with)&quot;)])

#create MSA and distance function
MSA &lt;- msa(inputSeqs = Fasta, method = &quot;$(Method)&quot;, order = &quot;input&quot;)
	
MSAconv &lt;- msaConvert(MSA, type=&quot;seqinr::alignment&quot;)
	
d &lt;- dist.alignment(MSAconv, &quot;identity&quot;)



if (tolower(&quot;$(PlotTree)&quot;) == &quot;true&quot;){
  #required for beauty plots and phylo subsets
  #is loaded in here so that you don&apos;t need to install them on server
  #if you don&apos;t want to plot
  library(treeio)
  library(ggplot2)
  library(ggtree)

  #create tree
  PhyloTree &lt;- nj(d)

  #save minimal tree
  p &lt;- ggtree(PhyloTree, layout = &apos;$(Plot Type)&apos;, branch.length= if (tolower(&quot;$(Real Distance)&quot;) == &quot;true&quot;){&quot;branch.length&quot;} else {&apos;none&apos;})

  if (tolower(&quot;$(Color Tree)&quot;) == &quot;true&quot;){
	#create groups to color by and use tipnames to color the right tips
	tipnames &lt;- as.matrix(AllData[c(&quot;$(Annotate with)&quot;)])
	groups &lt;- unique(AllData[c(&quot;$(Color By)&quot;)])
	groups &lt;- sapply(groups, as.character)
	grp &lt;- vector(mode=&quot;list&quot;, length=length(groups))
	names(grp) &lt;- as.character(groups)
	for (i in 1:length(groups)){
	  index &lt;- groups[i]
	  grp[[index]] &lt;- tipnames[tipnames %in% AllData[AllData[,grep(&quot;$(Color By)&quot;, colnames(AllData))] %in% groups[i],grep(&quot;$(Annotate with)&quot;, colnames(AllData))]]
	}

	#add colorized groups to plot
	p &lt;- groupOTU(p, grp, &apos;$(Color By)&apos;) + aes(color=$(Color By) ) + theme(legend.position=&quot;right&quot;)
	#if not circular add straight tiplabels
	if (tolower(&quot;$(Plot Type)&quot;) != &quot;circular&quot;){
	  p &lt;- p + geom_tiplab(aes(color = $(Color By))) 
	} else {
	  p &lt;- p + geom_tiplab2(aes(color = $(Color By))) 
	}
  } else {
	#if not add generic tip labels
	p &lt;- p + geom_tiplab2()
  }

  if (tolower(&quot;$(Level Back)&quot;) == &quot;true&quot;){
	#define maximum level
	maxLevel &lt;- FALSE
	i &lt;- 1
	tipsprevious &lt;- c(&quot;&quot;)
	while (maxLevel != TRUE){
	  tipsnew &lt;- tree_subset(PhyloTree, &quot;$(Ancestors Of)&quot;, levels_back = i)$tip.label
	  if (!(length(tipsprevious) &lt; length(tipsnew))){
		maxLevel &lt;- TRUE
		max_levels_back &lt;- i-1
	  }else{
		i &lt;- i + 1
		tipsprevious &lt;- tipsnew
	  }
	}
	max_levels_back

	#save nodes where each level starts
	subset &lt;- tree_subset(PhyloTree, &quot;$(Ancestors Of)&quot;, levels_back = 1)
	mrca_node &lt;- getMRCA(PhyloTree, tip = c(subset$tip.label))
	for (i in 2:max_levels_back){
	  subset &lt;- tree_subset(PhyloTree, &quot;$(Ancestors Of)&quot;, levels_back = i)
	  print(subset$tip.label)
	  mrca_node &lt;- cbind(mrca_node, getMRCA(PhyloTree, tip = c(subset$tip.label)))
	}
	
	#color by node and add to plot 
	colfunc &lt;- colorRampPalette(c(&quot;#4F81BD&quot;, &quot;#8064A2&quot;))
	colors &lt;- colfunc(length(mrca_node))
	for (i in length(mrca_node):1){
	  print(i)
	  print(mrca_node[i])
	  p &lt;- p + geom_balance(node=mrca_node[i], fill=colors[1], color=&apos;white&apos;, alpha=.6)
	  #p &lt;- p + geom_cladelabel(node=mrca_node[i], label=&apos;yoyo&apos;, fontsize=12, align=TRUE, color=&quot;firebrick&quot;)
	  #p &lt;- p + geom_point2(aes(subset=(node==mrca_node[i])), shape=23, size=5, fill=&apos;red&apos;)
	  #substitute(geom_point2(aes(subset=(node==mrca_node)), shape=23, size=5, fill=&apos;red&apos;), p)
	}
  } 
  
  #print the final plot to fail port
  #tryout
  par(mar=c(5,6,4,1)+.1)
  p
}

#what to output to pass
if (&quot;$(ROutput)&quot; == &quot;Keep Original&quot;) {
  AllData &lt;- c(&quot;&quot;)
}else if (&quot;$(ROutput)&quot; == &quot;Append Aligned Sequence&quot;){
  if (TRUE){
	library(caret)
	#remove near zero variance parameters
	MSA &lt;- as.matrix(MSA)[,!nearZeroVar(as.matrix(MSA), freqCut = 50/50, saveMetrics = TRUE)$nzv]
  }
  AlignedSequence &lt;- matrix()
  for (i in 1:length(as.matrix(MSA)[,1])){
    AlignedSequence &lt;- cbind(AlignedSequence, as.matrix(paste(as.matrix(MSA)[i,], collapse=&quot;&quot;)))
  }
  AllData &lt;- t(AlignedSequence)[-1,]
}else if (&quot;$(ROutput)&quot; == &quot;Append Alignment&quot;) {
  AllData &lt;- as.matrix(MSA)
}else if (&quot;$(ROutput)&quot; == &quot;Append Distance Score&quot;) {
  AllData &lt;- as.matrix(d)
}else if (&quot;$(ROutput)&quot; == &quot;Select Layer&quot;) {
  PhyloTree &lt;- nj(d)
  subset &lt;- tree_subset(PhyloTree, &quot;$(Ancestors Of)&quot;, levels_back = $(Levels Back))
  AllData &lt;- (subset$tip.label)
}

	

	



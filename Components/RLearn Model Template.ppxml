								<sci:value>@&apos;File Names&apos; := &quot;&quot;;
@&apos;File Values&apos; := &quot;&quot;;

for #i in 1 .. numGlobalProperties() loop
	if contains(@TemporaryPredictionFiles, ithGlobalPropertyName(#i)) then
		append(@&apos;File Names&apos; , ithGlobalPropertyName(#i));
		append(@&apos;File Values&apos; , ithGlobalProperty(#i));
	end if;
end loop;

property(&apos;GlobalNames&apos;) := @&apos;File Names&apos;;
property(&apos;FileURLs&apos;) := @&apos;File Values&apos;;	

